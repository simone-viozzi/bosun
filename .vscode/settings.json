{
    "go.alternateTools": {
        "dlv": "${workspaceFolder}/.direnv/go-tools/delve/dlv"
    },
    "gopls": {
        "ui.semanticTokens": true,
        "build.buildFlags": ["-tags=integration"]
    },
    "github.copilot.chat.commitMessageGeneration.instructions": [
        {
            "file": ".github/instructions/commit-msg.instructions.md"
        }
    ],
    "makefile.configureOnOpen": false,
    "gitlens.plusFeatures.enabled": false,
    "gitlens.gitkraken.mcp.autoEnabled": false,
    "gitlens.graph.statusBar.enabled": false,
    "gitlens.telemetry.enabled": false,
    "gitlens.ai.enabled": false,
}
